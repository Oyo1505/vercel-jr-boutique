    "postcss": "^8.4.24",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.1",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
