{
  "private": true,
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "lintFix": "eslint \"***/**/*.{tsx,ts,js}\" --fix && stylelint \"***/**/*.scss\" --fix",
